{
  "name": "hana-cli",
  "version": "3.202602.0",
  "description": "HANA Developer Command Line Interface",
  "main": "index.js",
  "bin": {
    "hana-cli": "./bin/cli.js"
  },
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "test": "mocha --config=tests/.mocharc.json \"./tests/**/*.Test.js\"",
    "test:cli": "mocha --config=tests/.mocharc.json \"./tests/*.Test.js\"",
    "test:utils": "mocha --config=tests/.mocharc.json \"./tests/utils/*.Test.js\"",
    "test:routes": "mocha --config=tests/.mocharc.json \"./tests/routes/*.Test.js\"",
    "changelog": "node CHANGELOG",
    "types": "tsc  --declaration --allowJs --emitDeclarationOnly --outDir types --skipLibCheck",
    "postinstall": "npm link @sap/cds-dk --local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAP-samples/hana-developer-cli-tool-example.git"
  },
  "keywords": [
    "hana",
    "sap",
    "database",
    "cli"
  ],
  "types": "types/index.d.ts",
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.9",
    "@cap-js/graphql": "0.14.0",
    "@cap-js/hana": "^2.6.0",
    "@cap-js/postgres": "^2.1.3",
    "@cap-js/sqlite": "^2.1.3",
    "@cap-js/telemetry": "^1.6.0",
    "@inquirer/prompts": "^8.2.0",
    "@json2csv/node": "7.0.6",
    "@sap-cloud-sdk/resilience": "^4.4.0",
    "@sap/cds": "^9.7.1",
    "@sap/cds-common-content": "^3.1.0",
    "@sap/cds-fiori": "^2.2.0",
    "@sap/cds-hana": "^2.1.0",
    "@sap/textbundle": "^6.1.0",
    "@sap/xsenv": "^6.1.0",
    "body-parser": "^2.2.2",
    "cds-swagger-ui-express": "^0.10.0",
    "chalk": "^5.3.0",
    "check-node-version": "^4.2.1",
    "cli-cursor": "^5.0.0",
    "cli-highlight": "^2.1.11",
    "debug": "^4.3.4",
    "dotenv": "^17.2.4",
    "easy-table": "1.2.0",
    "express": "^5.2.1",
    "figures": "^6.1.0",
    "glob": "^13.0.1",
    "hdb": "2.27.1",
    "inquirer": "^13.2.2",
    "js-convert-case": "^4.2.0",
    "js-yaml": "^4.1.0",
    "json-to-table": "^4.2.1",
    "latest-version": "^9.0",
    "lodash": "^4.17.23",
    "marked": "^17.0.1",
    "marked-terminal": "^7.0",
    "jszip": "^3.10.1",
    "odata2openapi": "^1.3.2",
    "open": "^11.0.0",
    "ora": "^9.3.0",
    "prompt": "^1.3.0",
    "properties-to-json": "^0.2.1",
    "rxjs": "^7.8.1",
    "sap-hdb-promisfied": "2.202601.0",
    "showdown": "^2.1.0",
    "swagger-ui-express": "^5.0.0",
    "terminal-kit": "^3.1.1",
    "terminaltools": "3.0.1-beta",
    "upath": "^2.0.1",
    "update-notifier": "^7.0.0",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "yargs": "^18.0.0"
  },
  "peerDependencies": {
    "@sap/cds-dk": ">=9.7.1"
  },
  "author": "SAP",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/SAP-samples/hana-developer-cli-tool-example/issues"
  },
  "homepage": "https://github.com/SAP-samples/hana-developer-cli-tool-example#readme",
  "devDependencies": {
    "@sap/eslint-plugin-cds": "^4.1.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.2.2",
    "@types/showdown": "^2.0.4",
    "@types/yargs": "^17.0.31",
    "eslint": "^9.39.2",
    "mochawesome": "^7.1.3",
    "sinon": "^21.0.1",
    "stringify-changelog": "^0.2.1"
  },
  "cds": {
    "preview": {
      "ui5": {
        "version": "1.144.1"
      }
    },
    "hana": {
      "journal": {
        "enable-drop": false,
        "change-mode": "alter"
      },
      "deploy-format": "hdbtable"
    },
    "requires": {
      "middlewares": true,
      "db": {
        "kind": "hana",
        "impl": "@cap-js/hana"
      }
    },
    "features": {
      "preserve_computed": true,
      "kibana_formatter": true,
      "audit_personal_data": true,
      "optimized_search": true,
      "rest_new_adapter": true,
      "odata_new_parser": true
    },
    "protocols": {
      "graphql": {
        "path": "/graphql",
        "impl": "@cap-js/graphql"
      }
    },
    "log": {
      "service": true
    }
  },
  "overrides": {
    "marked-terminal": {
      "marked": "^17.0.1"
    },
    "mocha": {
      "diff": "^8.0.3"
    }
  }
}
