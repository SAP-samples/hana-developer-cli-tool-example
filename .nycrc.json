{
  "all": true,
  "include": [
    "bin/**/*.js",
    "utils/**/*.js",
    "routes/**/*.js",
    "index.js"
  ],
  "exclude": [
    "node_modules/**",
    "tests/**",
    "coverage/**",
    ".nyc_output/**",
    "mochawesome-report/**",
    "types/**",
    "app/**",
    "**/*.Test.js",
    "CHANGELOG.js",
    "update-notifier.js"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "check-coverage": false,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "cache": true,
  "sourceMap": false,
  "instrument": true
}
